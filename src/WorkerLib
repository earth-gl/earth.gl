import { LongtermSupportWorkerType, ShorttermSupportWorkerType } from "./WorkerProtocol";

const workerLib: Map<LongtermSupportWorkerType | ShorttermSupportWorkerType, string> = new Map();

//worker source text
const VectorTileWorker = require('./../../dist/worker/VectorTileWorker.txt') as string;
// const TerrainTileWorker = require('./../../dist/worker/TerrainTileWorker.txt') as string;
const EParamterTileWorker = require('./../../dist/worker/EParameterWorker.txt') as string;

//lib
workerLib.set(LongtermSupportWorkerType.VectorTileWorker, URL.createObjectURL(new Blob([VectorTileWorker], { type: 'text/javascript' })));
// workerLib.set(LongtermSupportWorkerType.TerrainTileWorker, URL.createObjectURL(new Blob([TerrainTileWorker], { type: 'text/javascript' })))
workerLib.set(LongtermSupportWorkerType.EParamterTileWorker, URL.createObjectURL(new Blob([EParamterTileWorker], { type: 'text/javascript' })))

//short

export { workerLib }
